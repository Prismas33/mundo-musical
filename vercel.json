{
  "version": 3,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "outputDirectory": ".next"
      }
    }
  ],
  "regions": ["iad1"],
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=4096"
  },
  "functions": {
    "src/app/**": {
      "maxDuration": 30
    }
  }
}
